{
    "model_config": "guangyangmusic/legato-small",
    "pretrained_model": "outputs/legato-small-lr-3e-4/",

    "dataset_path": "datasets/PDMX-Synth",
    "mini_test_file": "datasets/mini_test.json",
    "dummy_data": false,

    "output_dir": "outputs/legato-small-lr-3e-4/",
    "remove_unused_columns": false,

    "run_name": "legato-small-predict",
    "do_train": false,
