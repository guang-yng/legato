    "dataloader_num_workers": 8,
    "dataloader_prefetch_factor": 2,
    "ddp_find_unused_parameters": false,
    "per_device_eval_batch_size": 2,
    "bf16_full_eval": true,
    "predict_with_generate": true,
    "generation_max_length": 2048,
    "generation_num_beams": 3,

    "bf16": true,
    "report_to": "wandb",
    "log_level": "info"
